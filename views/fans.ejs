<h2 id="fan-title" class="text-center">List Fans</h2>

<div class="container liste">
    <% fans.forEach(f => { %>
        <div class="fan">
            <p>
                <span class="item-label text-info text-uppercase">Name :</span>
                <%= f.name %>
            </p>
            <p>
                <span class="item-label text-info text-uppercase">Email :</span>
                <%= f.email %>
            </p>
        </div>
        <% }); %>
</div>