<div class="container">
  <div class="col-sm-6"><img src="/images/courses.jpg" alt="" /></div>
  <div class="col-sm-6">
    <h1 class="title">Learn to code with ES6</h1>
    <h3>Our Courses:</h3>
    <table class="table table-striped table-bordered">
      <thead>
        <tr>
          <th>Courses</th>
          <th>Cost (Euros)</th>
        </tr>
      </thead>
      <tbody>

        <% offredCourses.forEach((course)=> { %>
          <tr>
            <td>
              <%= course.title %>
            </td>
            <td>
              <%= course.cost %>
            </td>
          </tr>
          <% }); %>

      </tbody>
    </table>
    <a href="/contact">Contact Us</a> today!
  </div>
</div>